          </ul>
      </header>

      <body class="container">
        
       
           <table class="table table-dark table-striped table-bordered border-light table align-middle">
            <thead>
              <tr>
                <th scope="col-10 align-middle">ID</th>
                <th scope="col-10 align-middle">DESCRIPCION</th>
                <th scope="col-10">REFERENCIA</th>
                <th scope="col-10">UNIDAD EMPAQUE</th>
                <th scope="col-10">MARCA</th>
              </tr>
            </thead>
            <% productos.forEach(producto => { %>
            <tbody>
              <tr>
                <th scope="row"><%= producto.id %></th>
                <td><%= producto.descripcion %></td>
                <td><%= producto.referencia %></td>
                <td><%= producto.und_empaque %></td>
                <td><%= producto.marca %></td>
              </tr>
           <% }) %>
          </tbody>
           <div class="input-group">
            <select class="form-select" id="inputGroupSelect04" aria-label="Example select with button addon">77
              
              <option selected>Choose...</option>
              <% productos.forEach(producto => { %>
              <option value="<%= producto.id %>"><%= producto.descripcion %></option>
              <% }) %>
            </select>
            <button class="btn btn-outline-secondary" type="button">Button</button>
          </div>

       </body>


          <a href="/registros/productos">Registro</a>
                 
          <script type="text/javascript" src="../javascript/bootstrap.min.js"></script>
          <script type="text/javascript" src="../javascript/bootstrap.bundle.min.js"></script>
          <script type="text/javascript" src="../javascript/index.js"></script> 

    </html> 